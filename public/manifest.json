{
    "manifest_version": 2,
    "name": "rest-docs-toc-extension",
    "version": "0.0.1",
    "description": "make 'Table Of Contents(TOC)' to highlight current position and provide some additional guidance",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/16-gray.png",
            "48": "icons/48-gray.png",
            "128": "icons/128-gray.png"
        },
        "default_popup": "not-found.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "customStyle.css"
            ],
            "js": [
                "detector.js",
                "keyEventListener.js"
            ]
        }
    ]
}